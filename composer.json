{
    "name": "silverstripe/blowgun",
    "require": {
        "php": ">=5.6",
        "psr/log": "^1.0.0",
        "aws/aws-sdk-php": "^3.0",
        "symfony/process": "^3.3",
        "symfony/console": "^3.3",
        "monolog/monolog": "^1.13"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "friendsofphp/php-cs-fixer": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\BlowGun\\": "src/SilverStripe/BlowGun"
        }
    },
    "bin": [
        "bin/blowgun"
    ],
    "config": {
        "platform": {
            "php": "5.6.22"
        },
        "process-timeout": 600
    }
}

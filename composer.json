{
    "name": "silverstripe/blowgun",
    "require": {
        "php": ">=5.6",
        "psr/log": "^1.0.0",
        "aws/aws-sdk-php": "^2.8",
        "symfony/process": "^2.7",
        "symfony/console": "^2.7",
        "monolog/monolog": "^1.13",
        "friendsofphp/php-cs-fixer": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\BlowGun\\": "src/SilverStripe/BlowGun"
        }
    },
    "bin": [
        "bin/blowgun"
    ],
    "config": {
        "process-timeout": 600
    }
}
